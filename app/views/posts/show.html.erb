<div class="container">
  <div class="title mt-5">
    <h1><strong><%= @post.title %></strong></h1>
  </div>
  <%= render "shared/line" %>
  <div class="blog-photo mt-4 mb-4">
    <%= cl_image_tag @post.photo.key, crop: :mfit %>
  </div>
  <%= render "shared/line" %>
      <%= link_to "Edit", edit_post_path(@post)  %>
  <div class="content mt-5">
      <p><%= @post.content %></p>
  </div>
</div>
